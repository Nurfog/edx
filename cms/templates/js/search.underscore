<div id="videos-search-bar" class="search-bar videos-search-bar" role="search" aria-label="Video">
  <div class="search-field-wrapper">
    <input id="search-input" type="text" class="search-field"/>
    <button class="search-button"><%- gettext("Search") %></button>
    <button class="cancel-button" title="<%- gettext("Clear Search") %>">
      <span class="icon fa fa-remove" aria-hidden="true"></span>
    </button>
  </div>
</div>